  "brand_logic": {
    "dirco_presence_rule": "Recipient position. Top-left placement within header zone. Maximum height 22mm. Full colour only. Maintains government protocol primacy. No recolouring. Minimum clearspace equal to height of coat of arms shield.",
    "tirisano_presence_rule": "Bidder position. Lower zone placement. Full logo with wordmark for primary identification. Maximum height 18mm. Logomark used separately only for watermark treatment. Clearspace minimum 8mm all sides.",
    "hierarchy_order": ["DIRCO (recipient)", "Document classification", "Bid title", "Tender reference", "Closing deadline", "Bidder identity", "Contact details"],
    "tone_keywords": ["diplomatic", "official", "confidential", "premium African identity", "audit-ready", "protocol-driven", "institutional gravitas"]
  },
  "palette": [
    { "name": "tirisano_navy", "hex": "#1A2B5A", "role": "Primary text, accent lines, authority" },
    { "name": "tirisano_green", "hex": "#006B3F", "role": "Secondary accent, complements DIRCO green" },
    { "name": "tirisano_red", "hex": "#D32027", "role": "Tertiary accent, use sparingly" },
    { "name": "tirisano_gold", "hex": "#F4B223", "role": "Tertiary accent, use sparingly" },
    { "name": "neutral_dark", "hex": "#2D2D2D", "role": "Body text alternative" },
    { "name": "neutral_mid", "hex": "#6B6B6B", "role": "Secondary text, labels" },
