      </w:r>
      <w:r>
        <w:rPr>
          <w:rFonts w:ascii="Californian FB" w:h-ansi="Californian FB"/>
          <w:sz w:val="28"/><w:sz-cs w:val="28"/>
        </w:rPr>
        <w:t>, Pro se</w:t>
      </w:r>
    </w:p>

    <!-- ==================== PAGE SETTINGS ==================== -->
    <w:sectPr>
      <w:hdr w:type="odd">
        <w:p>
          <w:pPr><w:jc w:val="center"/></w:pPr>
          <w:r>
            <w:rPr>
              <w:rFonts w:ascii="Californian FB" w:h-ansi="Californian FB"/>
              <w:b/><w:sz w:val="28"/>
            </w:rPr>
            <w:t>Case No. {{CASE_NUMBER}}</w:t>
          </w:r>
        </w:p>
      </w:hdr>
      <w:ftr w:type="odd">
        <w:p>
          <w:pPr><w:jc w:val="center"/></w:pPr>
          <w:r><w:rPr><w:sz w:val="20"/></w:rPr><w:t>Page </w:t></w:r>
          <w:r><w:rPr><w:sz w:val="20"/></w:rPr><w:fldChar w:fldCharType="begin"/></w:r>
          <w:r><w:rPr><w:sz w:val="20"/></w:rPr><w:instrText>PAGE</w:instrText></w:r>
          <w:r><w:rPr><w:sz w:val="20"/></w:rPr><w:fldChar w:fldCharType="separate"/></w:r>
          <w:r><w:rPr><w:sz w:val="20"/></w:rPr><w:t>1</w:t></w:r>
