<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<?mso-application progid="Word.Document"?>
<w:wordDocument xmlns:w="http://schemas.microsoft.com/office/word/2003/wordml" xmlns:wx="http://schemas.microsoft.com/office/word/2003/auxHint" xmlns:o="urn:schemas-microsoft-com:office:office" xml:space="preserve">

<!-- ============================================
     NOTICE TEMPLATE - PIMP SMACK WORKFLOW
     Placeholders: {{PLACEHOLDER_NAME}}
     ============================================ -->

<w:styles>
  <w:style w:type="paragraph" w:default="on" w:styleId="Normal">
    <w:name w:val="Normal"/>
    <w:rPr>
      <w:rFonts w:ascii="Times New Roman" w:h-ansi="Times New Roman"/>
      <w:sz w:val="24"/><w:sz-cs w:val="24"/>
    </w:rPr>
  </w:style>

  <w:style w:type="paragraph" w:styleId="Heading1">
    <w:name w:val="heading 1"/>
    <w:pPr>
      <w:spacing w:before="480" w:after="360"/>
      <w:jc w:val="center"/>
      <w:outlineLvl w:val="0"/>
    </w:pPr>
    <w:rPr>
      <w:rFonts w:ascii="Californian FB" w:h-ansi="Californian FB"/>
      <w:b/><w:b-cs/>
      <w:sz w:val="32"/><w:sz-cs w:val="32"/>
    </w:rPr>
  </w:style>
</w:styles>

<w:docPr>
  <w:view w:val="print"/>
  <w:zoom w:percent="100"/>
</w:docPr>

<w:body>
  <wx:sect>
    <!-- ==================== TITLE ==================== -->
    <w:p>
      <w:pPr><w:pStyle w:val="Heading1"/></w:pPr>
      <w:r><w:t>{{NOTICE_TITLE}}</w:t></w:r>
    </w:p>

    <!-- ==================== TO LINE ==================== -->
    <w:p>
      <w:pPr>
        <w:spacing w:after="240"/>
        <w:rPr>
          <w:rFonts w:ascii="Californian FB" w:h-ansi="Californian FB"/>
          <w:sz w:val="28"/><w:sz-cs w:val="28"/>
        </w:rPr>
      </w:pPr>
      <w:r>
        <w:rPr>
          <w:rFonts w:ascii="Californian FB" w:h-ansi="Californian FB"/>
          <w:b/><w:sz w:val="28"/><w:sz-cs w:val="28"/>
        </w:rPr>
        <w:t>TO:</w:t>
      </w:r>
      <w:r>
        <w:rPr>
          <w:rFonts w:ascii="Californian FB" w:h-ansi="Californian FB"/>
          <w:sz w:val="28"/><w:sz-cs w:val="28"/>
        </w:rPr>
        <w:tab/>
        <w:t>{{NOTICE_RECIPIENTS}}</w:t>
      </w:r>
    </w:p>

    <!-- ==================== PLEASE TAKE NOTICE ==================== -->
    <w:p>
      <w:pPr>
        <w:spacing w:before="360" w:after="240" w:line="480" w:line-rule="auto"/>
        <w:ind w:first-line="720"/>
        <w:rPr>
          <w:rFonts w:ascii="Californian FB" w:h-ansi="Californian FB"/>
          <w:sz w:val="28"/><w:sz-cs w:val="28"/>
        </w:rPr>
      </w:pPr>
      <w:r>
        <w:rPr>
          <w:rFonts w:ascii="Californian FB" w:h-ansi="Californian FB"/>
          <w:b/><w:sz w:val="28"/><w:sz-cs w:val="28"/>
        </w:rPr>
        <w:t>PLEASE TAKE NOTICE</w:t>
      </w:r>
      <w:r>
        <w:rPr>
          <w:rFonts w:ascii="Californian FB" w:h-ansi="Californian FB"/>
          <w:sz w:val="28"/><w:sz-cs w:val="28"/>
        </w:rPr>
        <w:t> that {{NOTICE_BODY}}</w:t>
      </w:r>
    </w:p>

    <!-- ==================== NOTICE DETAILS ==================== -->
    <w:p>
      <w:pPr>
        <w:spacing w:after="120"/>
        <w:ind w:left="720"/>
        <w:rPr>
          <w:rFonts w:ascii="Californian FB" w:h-ansi="Californian FB"/>
          <w:sz w:val="28"/><w:sz-cs w:val="28"/>
        </w:rPr>
      </w:pPr>
      <w:r>
        <w:rPr>
          <w:rFonts w:ascii="Californian FB" w:h-ansi="Californian FB"/>
          <w:b/><w:sz w:val="28"/><w:sz-cs w:val="28"/>
        </w:rPr>
        <w:t>Date:</w:t>
      </w:r>
      <w:r>
        <w:rPr>
          <w:rFonts w:ascii="Californian FB" w:h-ansi="Californian FB"/>
          <w:sz w:val="28"/><w:sz-cs w:val="28"/>
        </w:rPr>
        <w:tab/><w:tab/>
        <w:t>{{NOTICE_DATE}}</w:t>
      </w:r>
    </w:p>

    <w:p>
      <w:pPr>
        <w:spacing w:after="120"/>
        <w:ind w:left="720"/>
        <w:rPr>
          <w:rFonts w:ascii="Californian FB" w:h-ansi="Californian FB"/>
          <w:sz w:val="28"/><w:sz-cs w:val="28"/>
        </w:rPr>
      </w:pPr>
      <w:r>
        <w:rPr>
          <w:rFonts w:ascii="Californian FB" w:h-ansi="Californian FB"/>
          <w:b/><w:sz w:val="28"/><w:sz-cs w:val="28"/>
        </w:rPr>
        <w:t>Time:</w:t>
      </w:r>
      <w:r>
        <w:rPr>
          <w:rFonts w:ascii="Californian FB" w:h-ansi="Californian FB"/>
          <w:sz w:val="28"/><w:sz-cs w:val="28"/>
        </w:rPr>
        <w:tab/><w:tab/>
        <w:t>{{NOTICE_TIME}}</w:t>
      </w:r>
    </w:p>

    <w:p>
      <w:pPr>
        <w:spacing w:after="120"/>
        <w:ind w:left="720"/>
        <w:rPr>
          <w:rFonts w:ascii="Californian FB" w:h-ansi="Californian FB"/>
          <w:sz w:val="28"/><w:sz-cs w:val="28"/>
        </w:rPr>
      </w:pPr>
      <w:r>
        <w:rPr>
          <w:rFonts w:ascii="Californian FB" w:h-ansi="Californian FB"/>
          <w:b/><w:sz w:val="28"/><w:sz-cs w:val="28"/>
        </w:rPr>
        <w:t>Location:</w:t>
      </w:r>
      <w:r>
        <w:rPr>
          <w:rFonts w:ascii="Californian FB" w:h-ansi="Californian FB"/>
          <w:sz w:val="28"/><w:sz-cs w:val="28"/>
        </w:rPr>
        <w:tab/>
        <w:t>{{NOTICE_LOCATION}}</w:t>
      </w:r>
    </w:p>

    <w:p>
      <w:pPr>
        <w:spacing w:after="360"/>
        <w:ind w:left="720"/>
        <w:rPr>
          <w:rFonts w:ascii="Californian FB" w:h-ansi="Californian FB"/>
          <w:sz w:val="28"/><w:sz-cs w:val="28"/>
        </w:rPr>
      </w:pPr>
      <w:r>
        <w:rPr>
          <w:rFonts w:ascii="Californian FB" w:h-ansi="Californian FB"/>
          <w:b/><w:sz w:val="28"/><w:sz-cs w:val="28"/>
        </w:rPr>
        <w:t>Judge:</w:t>
      </w:r>
      <w:r>
        <w:rPr>
          <w:rFonts w:ascii="Californian FB" w:h-ansi="Californian FB"/>
          <w:sz w:val="28"/><w:sz-cs w:val="28"/>
        </w:rPr>
        <w:tab/><w:tab/>
        <w:t>{{JUDGE_NAME}}</w:t>
      </w:r>
    </w:p>

    <!-- ==================== ADDITIONAL NOTICE ==================== -->
    <w:p>
      <w:pPr>
        <w:spacing w:after="240" w:line="480" w:line-rule="auto"/>
        <w:ind w:first-line="720"/>
        <w:rPr>
          <w:rFonts w:ascii="Californian FB" w:h-ansi="Californian FB"/>
          <w:sz w:val="28"/><w:sz-cs w:val="28"/>
        </w:rPr>
      </w:pPr>
      <w:r>
        <w:rPr>
          <w:rFonts w:ascii="Californian FB" w:h-ansi="Californian FB"/>
          <w:sz w:val="28"/><w:sz-cs w:val="28"/>
        </w:rPr>
        <w:t>{{ADDITIONAL_NOTICE}}</w:t>
      </w:r>
    </w:p>

    <!-- ==================== DATED LINE ==================== -->
    <w:p>
      <w:pPr>
        <w:spacing w:before="360" w:after="120"/>
        <w:rPr>
          <w:rFonts w:ascii="Californian FB" w:h-ansi="Californian FB"/>
          <w:sz w:val="28"/><w:sz-cs w:val="28"/>
        </w:rPr>
      </w:pPr>
      <w:r>
        <w:rPr>
          <w:rFonts w:ascii="Californian FB" w:h-ansi="Californian FB"/>
          <w:sz w:val="28"/><w:sz-cs w:val="28"/>
        </w:rPr>
        <w:t>DATED this {{DAY}} day of {{MONTH}}, {{YEAR}}.</w:t>
      </w:r>
    </w:p>

    <!-- Respectfully submitted -->
    <w:p>
      <w:pPr>
        <w:spacing w:before="240" w:after="60"/>
        <w:rPr>
          <w:rFonts w:ascii="Californian FB" w:h-ansi="Californian FB"/>
          <w:sz w:val="28"/><w:sz-cs w:val="28"/>
        </w:rPr>
      </w:pPr>
      <w:r>
        <w:rPr>
          <w:rFonts w:ascii="Californian FB" w:h-ansi="Californian FB"/>
          <w:sz w:val="28"/><w:sz-cs w:val="28"/>
        </w:rPr>
        <w:t>Respectfully submitted,</w:t>
      </w:r>
    </w:p>

    <!-- ==================== SIGNATURE BLOCK ==================== -->
    <w:p>
      <w:pPr><w:ind w:left="4320"/></w:pPr>
      <w:r>
        <w:rPr>
          <w:rFonts w:ascii="Edwardian Script ITC" w:h-ansi="Edwardian Script ITC"/>
          <w:b/><w:b-cs/><w:i/><w:i-cs/>
          <w:sz w:val="52"/><w:sz-cs w:val="52"/>
          <w:u w:val="single"/>
        </w:rPr>
        <w:t>/s/ {{PARTY_NAME}}</w:t>
      </w:r>
    </w:p>

    <w:p>
      <w:pPr><w:ind w:left="4320"/></w:pPr>
      <w:r>
        <w:rPr>
          <w:rFonts w:ascii="Californian FB" w:h-ansi="Californian FB"/>
          <w:b/><w:b-cs/>
          <w:sz w:val="28"/><w:sz-cs w:val="28"/>
        </w:rPr>
        <w:t>{{PARTY_NAME_CAPS}}</w:t>
      </w:r>
      <w:r>
        <w:rPr>
          <w:rFonts w:ascii="Californian FB" w:h-ansi="Californian FB"/>
          <w:sz w:val="28"/><w:sz-cs w:val="28"/>
        </w:rPr>
        <w:t>, Pro se</w:t>
      </w:r>
    </w:p>

    <w:p>
      <w:pPr><w:ind w:left="4320"/></w:pPr>
      <w:r>
        <w:rPr>
          <w:rFonts w:ascii="Californian FB" w:h-ansi="Californian FB"/>
          <w:sz w:val="28"/><w:sz-cs w:val="28"/>
        </w:rPr>
        <w:t>{{ADDRESS_LINE_1}}</w:t>
      </w:r>
    </w:p>
    <w:p>
      <w:pPr><w:ind w:left="4320"/></w:pPr>
      <w:r>
        <w:rPr>
          <w:rFonts w:ascii="Californian FB" w:h-ansi="Californian FB"/>
          <w:sz w:val="28"/><w:sz-cs w:val="28"/>
        </w:rPr>
        <w:t>{{CITY_STATE_ZIP}}</w:t>
      </w:r>
    </w:p>
    <w:p>
      <w:pPr><w:ind w:left="4320"/></w:pPr>
      <w:r>
        <w:rPr>
          <w:rFonts w:ascii="Californian FB" w:h-ansi="Californian FB"/>
          <w:sz w:val="28"/><w:sz-cs w:val="28"/>
        </w:rPr>
        <w:t>Email - {{EMAIL}}</w:t>
      </w:r>
    </w:p>
    <w:p>
      <w:pPr><w:ind w:left="4320"/><w:spacing w:after="360"/></w:pPr>
      <w:r>
        <w:rPr>
          <w:rFonts w:ascii="Californian FB" w:h-ansi="Californian FB"/>
          <w:sz w:val="28"/><w:sz-cs w:val="28"/>
        </w:rPr>
        <w:t>Phone - {{PHONE}}</w:t>
      </w:r>
    </w:p>

    <!-- ==================== CERTIFICATE OF SERVICE ==================== -->
    <w:p>
      <w:pPr><w:pStyle w:val="Heading1"/></w:pPr>
      <w:r><w:t>CERTIFICATE OF SERVICE</w:t></w:r>
    </w:p>
    <w:p>
      <w:pPr>
        <w:spacing w:after="240" w:line="480" w:line-rule="auto"/>
        <w:ind w:first-line="720"/>
        <w:rPr>
          <w:rFonts w:ascii="Californian FB" w:h-ansi="Californian FB"/>
          <w:sz w:val="28"/><w:sz-cs w:val="28"/>
        </w:rPr>
      </w:pPr>
      <w:r>
        <w:rPr>
          <w:rFonts w:ascii="Californian FB" w:h-ansi="Californian FB"/>
          <w:sz w:val="28"/><w:sz-cs w:val="28"/>
        </w:rPr>
        <w:t>I hereby certify that on {{SERVICE_DATE}}, I electronically filed the foregoing {{NOTICE_TITLE}} with the Clerk of the Court using the CM/ECF system, which will send notification of such filing to all counsel of record.</w:t>
      </w:r>
    </w:p>

    <w:p>
      <w:pPr><w:ind w:left="4320"/></w:pPr>
      <w:r>
        <w:rPr>
          <w:rFonts w:ascii="Edwardian Script ITC" w:h-ansi="Edwardian Script ITC"/>
          <w:b/><w:b-cs/><w:i/><w:i-cs/>
          <w:sz w:val="52"/><w:sz-cs w:val="52"/>
          <w:u w:val="single"/>
        </w:rPr>
        <w:t>/s/ {{PARTY_NAME}}</w:t>
      </w:r>
    </w:p>
    <w:p>
      <w:pPr><w:ind w:left="4320"/></w:pPr>
      <w:r>
        <w:rPr>
          <w:rFonts w:ascii="Californian FB" w:h-ansi="Californian FB"/>
          <w:b/><w:b-cs/>
          <w:sz w:val="28"/><w:sz-cs w:val="28"/>
        </w:rPr>
