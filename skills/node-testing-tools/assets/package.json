{
  "name": "my-project",
  "version": "1.0.0",
  "description": "My Node.js/TypeScript project with comprehensive testing",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:cov": "vitest run --coverage",
    "test:debug": "vitest --inspect-brk --inspect --single-thread",
    "test:unit": "vitest run tests/unit/",
    "test:integration": "vitest run tests/integration/",
    "e2e": "playwright test",
    "e2e:ui": "playwright test --ui",
    "e2e:debug": "playwright test --debug",
    "arch": "depcruise --config .dependency-cruiser.cjs src",
    "knip": "knip",
    "lint": "biome lint .",
    "format": "biome format --write .",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {},
  "devDependencies": {
    "@biomejs/biome": "^1.5.0",
    "@playwright/test": "^1.40.0",
    "@testing-library/dom": "^9.3.0",
    "@testing-library/react": "^14.1.0",
    "@testing-library/svelte": "^4.0.0",
    "@testing-library/user-event": "^14.5.0",
    "@testing-library/vue": "^8.0.0",
    "@vitejs/plugin-react": "^4.2.0",
    "@vitest/coverage-v8": "^1.0.0",
    "@vitest/environment-jsdom": "^1.0.0",
    "@vitest/ui": "^1.0.0",
    "dependency-cruiser": "^13.1.0",
    "fast-check": "^3.13.0",
    "knip": "^3.11.0",
    "msw": "^2.0.0",
    "node-mocks-http": "^1.13.0",
    "supertest": "^6.3.0",
    "testcontainers": "^10.0.0",
    "typescript": "^5.3.0",
    "vite": "^5.0.0",
    "vitest": "^1.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
